<h1>BURGER TYPE</h1>
<div class="burgercontainer">
<ul class="left">
	{{#each burgers}} {{#unless devoured}} <li>
		
		{{burger_name}}

		<button class="dev" id={{id}} type="button">DEVOURED!</button>
		
	</li> {{/unless}} 
	 {{/each}}
</ul>
<ul class="right">
	{{#each burgers}} 
	{{#if devoured}}
	<li>{{burger_name}}</li> {{/if}} {{/each}}

</ul>
</div>
<form class="create-form">
	<div class="form-group">
		<input type="text" id="ca" name="name">
	</div>
	<button type="submit">ADD ONE</button>
</form>